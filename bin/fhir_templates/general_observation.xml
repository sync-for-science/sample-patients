  {%- extends "entry.xml" %}
  {%- block content %}
  <Observation xmlns="http://hl7.org/fhir">
    <text>
      <status value="generated"/>
      <div xmlns="http://www.w3.org/1999/xhtml">{{o.name}}</div>
    </text>
    <name>
      <coding>
        <system value="{{o.system}}"/>
        <code value="{{o.code}}"/>
        <display value="{{o.name}}"/>
      </coding>
    </name>
    <appliesDateTime value="{{o.date}}"/>
    <status value="final"/>
    <reliability value="ok"/>
    <subject>
      <reference value="{{pid}}"/>
    </subject>
  </Observation>
  {% endblock %}
